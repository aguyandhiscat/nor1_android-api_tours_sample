              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_duration"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_duration_minutes"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_frequency"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_pickup_required"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_dropoff_required"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_child_price"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
    <TextView android:id="@+id/item_schedule_adult_price"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" />
</LinearLayout>